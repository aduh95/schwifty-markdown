{
  "name": "schwifty-markdown",
  "description": "Convert Markdown documents to HTML on the fly, including yUML and PlantUML diagrams and graph charts rendering.",
  "version": "1.5.2",
  "keywords": [
    "markdown",
    "toc",
    "table of content",
    "plantuml"
  ],
  "author": {
    "email": "duhamelantoine1995@gmail.com",
    "name": "Antoine du HAMEL",
    "url": "https://aduh95.github.io"
  },
  "license": "MIT",
  "main": "index.mjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aduh95/schwifty-markdown.git"
  },
  "bugs": {
    "url": "https://github.com/aduh95/schwifty-markdown/issues"
  },
  "bin": {
    "schwifty": "bin/schwifty.js"
  },
  "dependencies": {
    "exitzero": "^1.0.1",
    "express": "expressjs/express#5.0",
    "fs-extra": "^5.0.0",
    "is-port-available": "^0.1.5",
    "is-windows": "^1.0.1",
    "jsdom": "^11.4.0",
    "marked": "^0.3.6",
    "mime": "^2.0.3",
    "node-plantuml": "^0.5.0",
    "open": "^0.0.5",
    "papaparse": "^4.3.6",
    "temp": "^0.8.3",
    "websocket": "^1.0.24",
    "yargs": "^10.0.1",
    "yuml2svg": "^3.0.0"
  },
  "scripts": {
    "start": "node src/checkDependencies.js && node --experimental-modules index.mjs",
    "test": "node tests/bootstrap.js",
    "preinstall": "node src/checkDependencies.js",
    "postinstall": "npx exitzero node bin/schwifty.js -u",
    "updateDependencies": "node --experimental-modules src/updateDependencies.mjs",
    "preversion": "yarn test",
    "postversion": "git push && git push --tags"
  },
  "engines": {
    "node": ">=8.5.0"
  },
  "devDependencies": {
    "cypress": "^1.1.0"
  },
  "yargs": {
    "boolean-negation": false
  }
